<configuration>
    <com>
        <pearson>
            <qa>
                <bluesky>
                    <csg>
                        <authentication>
                            <login>
                                <contexts>
                                    <LoginContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/authentication/login</path>
                                    </LoginContext>
                                </contexts>
                            </login>
                            <getSession>
                                <contexts>
                                    <GetSessionContext>
                                        <scheme>http</scheme>
                                    </GetSessionContext>
                                </contexts>
                            </getSession>
                            <profile>
                                <contexts>
                                    <ProfileContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/authentication/profile</path>
                                    </ProfileContext>
                                </contexts>
                            </profile>
                            <loginAs>
                                <contexts>
                                    <LoginAsContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/authentication/loginas</path>
                                    </LoginAsContext>
                                </contexts>
                            </loginAs>
                            <logout>
                                <contexts>
                                    <LogoutContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/authentication/logout</path>
                                    </LogoutContext>
                                </contexts>
                            </logout>
                        </authentication>
                        <collections>
                            <createcollection>
                                 <contexts>
                                <CreateCollectionContexts>
                                    <scheme>http</scheme>
                                    <path>/csg/api/v2/content/collections</path>
                                </CreateCollectionContexts>
                                 </contexts>
                            </createcollection>
                        </collections>
                        <books>
                            <createOrderHeader>
                                <contexts>
                                    <CreateOrderHeaderContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/orders</path>
                                    </CreateOrderHeaderContext>
                                </contexts>
                            </createOrderHeader>
                            <createOrderDetail>
                                <contexts>
                                    <CreateOrderDetailContext>
                                        <scheme>http</scheme>
                                    </CreateOrderDetailContext>
                                </contexts>
                            </createOrderDetail>
                            <bookPrice>
                                <contexts>
                                     <BookPriceContext>
                                         <scheme>http</scheme>
                                     </BookPriceContext>
                                </contexts>
                            </bookPrice>
                            <chapterPrice>
                                <contexts>
                                    <ChapterPriceContext>
                                        <scheme>http</scheme>
                                    </ChapterPriceContext>
                                </contexts>
                            </chapterPrice>
                            <registeredBooks>
                                <contexts>
                                    <RegisteredBooksContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/registered</path>
                                    </RegisteredBooksContext>
                                </contexts>
                            </registeredBooks>
                            <territories>
                                <contexts>
                                    <TerritoriesContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/territories </path>
                                    </TerritoriesContext>
                                </contexts>
                            </territories>
                            <schoolTerritoryAssociation>
                                <contexts>
                                    <SchoolTerritoryAssociationContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/territories/schools</path>
                                    </SchoolTerritoryAssociationContext>
                                </contexts>
                            </schoolTerritoryAssociation>
                            <bookBuild>
                                <contexts>
                                      <BookBuildContext>
                                          <scheme>http</scheme>
                                          <path>/csg/api/v2/books/</path>
                                      </BookBuildContext>
                                </contexts>
                            </bookBuild>
                            <approveISBNs>
                                <contexts>
                                    <ApproveIsbnContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/isbns/approve</path>
                                    </ApproveIsbnContext>
                                </contexts>
                            </approveISBNs>
                            <updateIsbnStatus>
                                <contexts>
                                    <UpdateIsbnStatusContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/isbn/status</path>
                                    </UpdateIsbnStatusContext>
                                </contexts>
                            </updateIsbnStatus>
                            <processCccOrder>
                                <contexts>
                                    <ProcessCccOrderContext>
                                        <scheme>http</scheme>
                                        <path>/csg/api/v2/books/processCCCOrder</path>
                                    </ProcessCccOrderContext>
                                </contexts>
                            </processCccOrder>
                        </books>
                    </csg>
                </bluesky>
            </qa>
        </pearson>
    </com>
</configuration>
